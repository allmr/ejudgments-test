{
  "name": "allmr-offline",
  "author": {
    "name": "ALL-MR",
    "email": "help@nearlaw.com"
  },
  "version": "1.0.0",
  "main": "./src/electron.js",
  "scripts": {
    "build": "rollup -c",
    "build-aio": "run-s createCitationDataAio createEquivalentCitationDataAio createCasesCitedDataAio createPartyNameDataAio createPetitionerDataAio createRespondentDataAio build cp-aio",
    "cp-aio": "cp -avr ./public/build/ ./public/build-aio/",
    "createCitationDataAio": "node createCitationData.js aio",
    "createEquivalentCitationDataAio": "node createEquivalentCitationData.js aio",
    "createCasesCitedDataAio": "node createCasesCitedData.js aio",
    "createPartyNameDataAio": "node createPartyNameData.js aio",
    "createPetitionerDataAio": "node createPetitionerData.js aio",
    "createRespondentDataAio": "node createRespondentData.js aio",
    "build-civil": "run-s createCitationDataCivil createEquivalentCitationDataCivil createCasesCitedDataCivil createPartyNameDataCivil build cp-civil",
    "cp-civil": "cp -avr ./public/build/ ./public/build-civil/",
    "createCitationDataCivil": "node createCitationData.js civil",
    "createEquivalentCitationDataCivil": "node createEquivalentCitationData.js civil",
    "createCasesCitedDataCivil": "node createCasesCitedData.js civil",
    "createPartyNameDataCivil": "node createPartyNameData.js civil",
    "build-criminal": "run-s createCitationDataCriminal createEquivalentCitationDataCriminal createCasesCitedDataCriminal createPartyNameDataCriminal build cp-criminal",
    "cp-criminal": "cp -avr ./public/build/ ./public/build-criminal/",
    "createCitationDataCriminal": "node createCitationData.js criminal",
    "createEquivalentCitationDataCriminal": "node createEquivalentCitationData.js criminal",
    "createCasesCitedDataCriminal": "node createCasesCitedData.js criminal",
    "createPartyNameDataCriminal": "node createPartyNameData.js criminal",
    "dev": "rollup -c -w",
    "start": "sirv public --quiet --host 127.0.0.1 --port 5000",
    "start:https": "sirv public --quiet  --port 5000 --host 127.0.0.1 --ssl --cert /etc/letsencrypt/live/ejudgments.com/fullchain.pem --key /etc/letsencrypt/live/ejudgments.com/privkey.pem",
    "start:dev": "sirv public --quiet --host 127.0.0.1 --dev -s",
    "electron": "run-s build pure-electron",
    "pure-electron": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "pack-win": "electron-builder --dir --win --ia32",
    "dist-win": "electron-builder --win",
    "cp": "cp -avr ./public/build/ ../ALL-ME-NW/dist/ALL-MR/ALLMR-criminal-x64/src/build/"
  },
  "build": {
    "appId": "com.example.app",
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@zerodevx/svelte-toast": "^0.6.3",
    "electron": "^9.0.5",
    "electron-builder": "^22.4.1",
    "mark.js": "^8.11.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.34",
    "postcss-load-config": "^2.1.0",
    "rollup": "^1.32.0",
    "rollup-plugin-css-only": "^2.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-purgecss": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.17.0",
    "svelte-paginate": "0.0.1",
    "svelte-preprocess": "^3.3.0",
    "svelte-scrollto": "^0.2.0",
    "tailwindcss": "^1.1.4"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.5.0",
    "@fortawesome/free-regular-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@types/pouchdb-browser": "^6.1.3",
    "axios": "^0.19.2",
    "electron-find": "^1.0.6",
    "file-saver": "^2.0.5",
    "flexsearch": "^0.6.32",
    "fontawesome-svelte": "^2.0.1",
    "pdfjs-dist": "^2.4.456",
    "pouchdb": "^7.2.1",
    "pouchdb-browser": "^7.2.1",
    "pouchdb-find": "^7.2.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.8",
    "sirv-cli": "^0.4.4",
    "sv-bootstrap-dropdown": "^0.4.4",
    "svelte-accordion": "0.0.0-development",
    "svelte-fa": "^2.4.0",
    "svelte-pdf": "^1.0.24",
    "svelte-select": "^3.7.1",
    "svelte-simple-modal": "^0.3.0",
    "svelte-spa-router": "^2.0.1",
    "svelte-tablesort": "^1.1.0",
    "svelte-toast": "^1.0.0",
    "sveltestrap": "^3.3.0",
    "v8-compile-cache": "^2.1.0"
  }
}
